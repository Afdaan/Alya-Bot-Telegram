# Telegram Bot Token
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Gemini API Keys - Supports multiple API keys for rotation
GEMINI_API_KEY=your_primary_gemini_api_key_here
# Backup keys for rotate gemini api, separated by commas
GEMINI_BACKUP_API_KEYS=backup_key1,backup_key2,backup_key3

# Google Custom Search API - Multiple Keys
GOOGLE_SEARCH_API_KEY=your_primary_google_api_key_here
GOOGLE_SEARCH_API_KEY_2=your_second_google_api_key_here
GOOGLE_SEARCH_API_KEY_3=your_third_google_api_key_here
# Add more API keys as needed (up to GOOGLE_SEARCH_API_KEY_10)
GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id_here

# SauceNAO API Key (untuk reverse image search)
SAUCENAO_API_KEY=your_saucenao_api_key_here

# Developer Configuration
DEVELOPER_IDS=your_telegram_id