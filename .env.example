# Alya Bot Environment Configuration
# Copy this file to .env and update the values

# Required: Telegram Bot Token from BotFather
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Required: Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Required: Google Search API credentials
GOOGLE_SEARCH_API_KEY=your_google_search_api_key
GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id

# Required: SauceNAO API Key for reverse image search
SAUCENAO_API_KEY=your_saucenao_api_key

# Optional: Comma-separated list of admin user IDs 
DEVELOPER_IDS=123456789,987654321

# Optional: Backup Gemini API keys for high-traffic bots
GEMINI_BACKUP_API_KEYS=backup_key1,backup_key2,backup_key3